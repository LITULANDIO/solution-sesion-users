extends layout
block content
  
    .container 
    
    
      
    .col-md-6
      img(src='/img/search.png', width='26px', height='26px').ino 
      input#search.intros( type="text" placeholder="indica el lugar") 
      h1#title-mine.text-right
      .hr-line2
       each retiro in retiros
        div.over
        
          .col-sm-4.col-md-4.events-link
            .thumbnail.offer.margins.links.hvr-grow-shadow
             a.image-a(href='/details/'+ retiro.id)
              img(src= retiro.imageUrl, width="201px" height="174px" class="image2")
              
              h2#box.text-center
                |#{retiro.title} 
              div
                
                img.icon.icons8-Marker(src='data:image/png;base64,', width='26', height='26')
                | Localizaci√≥n: #{retiro.localitation}
                
                 
              br
              div
                img.icon.icons8-Calendar-Filled(src='data:image/png;base64,', width='26', height='26')
                |Fecha:#{retiro.startDate}
              p
                |
              hr
                
              div.iter
                div#lat(hidden) #{retiro.lat}
                div#long(hidden) #{retiro.long}
              img(src='https://maxcdn.icons8.com/Android_L/PNG/24/Ecommerce/price_tag-24.png', title='Price Tag', width='24')
              |   #{retiro.tags} 
    .col-md-6
      div#maps
    
      
    

            
              
      script(src='/js/allMaps.js')
      script(src='/js/filters.js')
          
      script(src = 'https://maps.googleapis.com/maps/api/js?key=AIzaSyD2CtWqsM0k23XnQMwJQXASOmNEkE8IAgM&callback=initMap')
      script(src='/js/insert-title.js')
           